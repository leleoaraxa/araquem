    ],
    "options": { "showHeader": true, "footer": { "show": true, "fields": "", "reducer": ["lastNotNull"] } },
    "fieldConfig": {
      "defaults": {
        "mappings": [],
        "thresholds": { "mode": "absolute", "steps": [{ "color": "green" }, { "color": "orange", "value": 200 }, { "color": "red", "value": 500 }] },
        "custom": { "align": "center", "displayMode": "auto" }
      },
      "overrides": [
        { "matcher": { "id": "byName", "options": "avg_latency_ms" }, "properties": [{ "id": "unit", "value": "ms" }] },
        { "matcher": { "id": "byName", "options": "cache_ratio" }, "properties": [{ "id": "unit", "value": "percentunit" }] },
        { "matcher": { "id": "byName", "options": "gold_agree_ratio" }, "properties": [{ "id": "unit", "value": "percentunit" }] }
