{
  "title": "20 · Planner & RAG Intelligence",
  "uid": "planner-rag-intelligence",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "1m",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "table",
      "title": "Top intents por volume",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [
        {
          "expr": "topk(10, sum(rate(api_requests_total{job=~'$job', entity=~'$entity'}[5m])) by (intent))",
          "format": "table",
          "refId": "A"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            }
          }
        }
      ]
    },
    {
      "id": 2,
      "type": "text",
      "title": "Planner score",
      "gridPos": {"h": 4, "w": 12, "x": 12, "y": 0},
      "options": {
        "content": "Planner score não instrumentado — adicione bindings em `data/ops/observability.yaml`.",
        "mode": "markdown"
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Misses / Unroutable (proxy)",
      "gridPos": {"h": 4, "w": 12, "x": 12, "y": 4},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [
        {
          "expr": "sum(rate(api_errors_total{job=~'$job', entity=~'$entity', intent=~'$intent'}[5m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "req/s"
        }
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "RAG · Docs e tamanho",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [
        {
          "expr": "sum(rag_index_docs_total{store=~'$store'})",
          "legendFormat": "Docs",
          "refId": "A"
        },
        {
          "expr": "sum(rag_index_size_total{store=~'$store'})",
          "legendFormat": "Tamanho (MB)",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Tamanho (MB)"},
            "properties": [
              {"id": "unit", "value": "decmbytes"}
            ]
          }
        ]
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "RAG · Density score",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [
        {
          "expr": "avg(rag_index_density_score{store=~'$store'})",
          "legendFormat": "Density",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red"},
              {"color": "green", "value": 10}
            ]
          }
        }
      }
    },
    {
      "id": 6,
      "type": "stat",
      "title": "Horas desde refresh",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 16},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [
        {
          "expr": "(time() - max(rag_index_last_refresh_timestamp{store=~'$store'})) / 3600",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "h",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green"},
              {"color": "red", "value": 24}
            ]
          }
        }
      }
    },
    {
      "id": 7,
      "type": "table",
      "title": "Density por store",
      "gridPos": {"h": 8, "w": 18, "x": 6, "y": 16},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [
        {
          "expr": "avg(rag_index_density_score{store=~'$store'}) by (store)",
          "format": "table",
          "refId": "A"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            }
          }
        }
      ]
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Correlação Latência x Density",
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(api_latency_ms_bucket{job=~'$job', entity=~'$entity'}[5m])) by (le, entity))",
          "legendFormat": "p95 {{entity}}",
          "refId": "A"
        },
        {
          "expr": "avg(rag_index_density_score{store=~'$store'})",
          "legendFormat": "Density média",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {"unit": "ms"},
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Density média"},
            "properties": [
              {"id": "unit", "value": "none"}
            ]
          }
        ]
      }
    },
    {
      "id": 90,
      "type": "stat",
      "title": "RAG · Recall@5",
      "gridPos": {"h": 4, "w": 4, "x": 0, "y": 32},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [{"expr": "rag_eval_recall_at_5", "refId": "A"}],
      "fieldConfig": { "defaults": { "unit": "percentunit" } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } },
      "description": "Binding: rag_eval_recall_at_5 · Threshold ref: 70"
    },
    {
      "id": 91,
      "type": "stat",
      "title": "RAG · Recall@10",
      "gridPos": {"h": 4, "w": 4, "x": 4, "y": 32},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [{"expr": "rag_eval_recall_at_10", "refId": "A"}],
      "fieldConfig": { "defaults": { "unit": "percentunit" } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } },
      "description": "Binding: rag_eval_recall_at_10 · Threshold ref: 70"
    },
    {
      "id": 92,
      "type": "stat",
      "title": "RAG · MRR",
      "gridPos": {"h": 4, "w": 4, "x": 8, "y": 32},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [{"expr": "rag_eval_mrr", "refId": "A"}],
      "fieldConfig": { "defaults": { "unit": "none" } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } },
      "description": "Binding: rag_eval_mrr · Threshold ref: 0.5"
    },
    {
      "id": 93,
      "type": "stat",
      "title": "RAG · nDCG@10",
      "gridPos": {"h": 4, "w": 4, "x": 12, "y": 32},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [{"expr": "rag_eval_ndcg_at_10", "refId": "A"}],
      "fieldConfig": { "defaults": { "unit": "none" } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } },
      "description": "Binding: rag_eval_ndcg_at_10 · Threshold ref: 0.6"
    },
    {
      "id": 94,
      "type": "stat",
      "title": "RAG · Last Eval (epoch)",
      "gridPos": {"h": 4, "w": 8, "x": 16, "y": 32},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [{"expr": "rag_eval_last_run_timestamp", "refId": "A"}],
      "fieldConfig": { "defaults": { "unit": "s" } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } },
      "description": "Binding: rag_eval_last_run_timestamp"
    }
  ],
  "templating": {
    "list": [
      {
        "name": "job",
        "type": "query",
        "datasource": {"type": "prometheus", "uid": "-"},
        "query": "label_values(api_requests_total, job)",
        "definition": "label_values(api_requests_total, job)",
        "includeAll": true,
        "refresh": 1,
        "current": {
          "selected": false,
          "text": "araquem-api",
          "value": "araquem-api"
        }
      },
      {
        "name": "entity",
        "type": "query",
        "datasource": {"type": "prometheus", "uid": "-"},
        "query": "label_values(api_requests_total, entity)",
        "definition": "label_values(api_requests_total, entity)",
        "includeAll": true,
        "refresh": 1,
        "current": {
          "selected": false,
          "text": "fiis_cadastro",
          "value": "fiis_cadastro"
        }
      },
      {
        "name": "store",
        "type": "query",
        "datasource": {"type": "prometheus", "uid": "-"},
        "query": "label_values(rag_index_docs_total, store)",
        "definition": "label_values(rag_index_docs_total, store)",
        "includeAll": true,
        "refresh": 1,
        "current": {
          "selected": false,
          "text": "embeddings.jsonl",
          "value": "embeddings.jsonl"
        }
      }
    ]
  }
}
