    {
      "type": "stat",
      "title": "Explain habilitado (requisições)",
      "id": 1,
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "options": {
        "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "expr": "sum(increase(sirios_planner_explain_enabled_total[5m]))",
          "legendFormat": "5m",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 1 },
              { "color": "red", "value": 10 }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Latência do explain (p50/p95/p99)",
      "id": 2,
      "gridPos": { "h": 6, "w": 18, "x": 6, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(sirios_planner_explain_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(sirios_planner_explain_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(sirios_planner_explain_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "lineWidth": 2 }
        },
        "overrides": []
      }
    },
    {
      "type": "bargauge",
      "title": "Nós contados no explain por tipo (5m)",
      "id": 3,
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 6 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "options": {
        "displayMode": "basic",
        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "sum(rate(sirios_planner_explain_nodes_total[5m])) by (node_kind)",
          "legendFormat": "{{node_kind}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 5 },
              { "color": "red", "value": 20 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Explain habilitado (rate)",
      "id": 4,
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 6 },
