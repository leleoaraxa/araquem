{
  "title": "Araquem — Quality Gates",
  "uid": "araquem-quality-gates",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "tags": [
    "Araquem",
    "Planner",
    "Quality",
    "YAML"
  ],
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "type": "gauge",
      "title": "Top-1 Accuracy",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 8,
        "h": 6
      },
      "targets": [
        {
          "expr": "sum(sirios_planner_top1_match_total{result=\"hit\"}) / clamp_min(sum(sirios_planner_top1_match_total), 1)",
          "legendFormat": ""
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "showThresholdLabels": true,
        "showThresholdMarkers": true,
        "min": 0,
        "max": 1,
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {
              "value": null,
              "color": "#E02F44"
            },
            {
              "value": 0.95,
              "color": "#299C46"
            }
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        },
        "overrides": []
      }
    },
    {
      "type": "gauge",
      "title": "Routed Rate",
      "gridPos": {
        "x": 8,
        "y": 0,
        "w": 8,
        "h": 6
      },
      "targets": [
        {
          "expr": "sum(sirios_planner_routed_total{outcome!=\"unroutable\"}) / clamp_min(sum(sirios_planner_routed_total), 1)",
          "legendFormat": ""
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "showThresholdLabels": true,
        "showThresholdMarkers": true,
        "min": 0,
        "max": 1,
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {
              "value": null,
              "color": "#E02F44"
            },
            {
              "value": 0.98,
              "color": "#299C46"
            }
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        },
        "overrides": []
      }
    },
    {
      "type": "gauge",
      "title": "Top-2 Gap (p50)",
      "gridPos": {
        "x": 16,
        "y": 0,
        "w": 8,
        "h": 6
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(sirios_planner_top2_gap_histogram_bucket[5m])) by (le))",
          "legendFormat": ""
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "showThresholdLabels": true,
        "showThresholdMarkers": true,
        "min": 0,
        "max": 1,
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {
              "value": null,
              "color": "#E02F44"
            },
            {
              "value": 0.4,
              "color": "#299C46"
            }
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Misses (abs)",
      "gridPos": {
        "x": 0,
        "y": 6,
        "w": 8,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(sirios_planner_top1_match_total{result=\"miss\"})"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Misses (ratio)",
      "gridPos": {
        "x": 8,
        "y": 6,
        "w": 8,
        "h": 4
      },
      "targets": [
        {
          "expr": "(sum(sirios_planner_top1_match_total{result=\"miss\"})) / clamp_min(sum(sirios_planner_top1_match_total), 1)"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        },
        "overrides": []
      }
    },
    {
      "type": "heatmap",
      "title": "Confusion Matrix — expected × predicted",
      "gridPos": {
        "x": 0,
        "y": 10,
        "w": 24,
        "h": 10
      },
      "targets": [
        {
          "expr": "sum by (expected_intent, predicted_intent) (sirios_planner_confusion_total)"
        }
      ],
      "options": {
        "yAxis": {
          "format": "short"
        }
      }
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  }
}
