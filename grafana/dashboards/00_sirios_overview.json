{
  "title": "00 · Sirios Overview",
  "uid": "sirios-overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "style": "dark",
  "refresh": "30s",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Tráfego (req/s)",
      "gridPos": {"h": 4, "w": 4, "x": 0, "y": 0},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [
        {
          "expr": "sum(rate(api_requests_total{job=~'$job', entity=~'$entity', intent=~'$intent'}[5m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "req/s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green"}
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Erros (%)",
      "gridPos": {"h": 4, "w": 4, "x": 4, "y": 0},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [
        {
          "expr": "100 * sum(rate(api_errors_total{job=~'$job', entity=~'$entity', intent=~'$intent'}[5m])) / sum(rate(api_requests_total{job=~'$job', entity=~'$entity', intent=~'$intent'}[5m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green"},
              {"color": "red", "value": 1.0}
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Latência p95 (ms)",
      "gridPos": {"h": 4, "w": 4, "x": 8, "y": 0},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(api_latency_ms_bucket{job=~'$job', entity=~'$entity', intent=~'$intent'}[5m])) by (le)) * 1000",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green"},
              {"color": "red", "value": 1500}
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Cache hit ratio (%)",
      "gridPos": {"h": 4, "w": 4, "x": 12, "y": 0},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [
        {
          "expr": "100 * sum(rate(cache_hits_total{entity=~'$entity'}[5m])) / (sum(rate(cache_hits_total{entity=~'$entity'}[5m])) + sum(rate(cache_misses_total{entity=~'$entity'}[5m])))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red"},
              {"color": "green", "value": 60}
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "RAG density",
      "gridPos": {"h": 4, "w": 4, "x": 16, "y": 0},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [
        {
          "expr": "avg(rag_index_density_score{store=~'$store'})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red"},
              {"color": "green", "value": 10}
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      }
    },
    {
      "id": 6,
      "type": "stat",
      "title": "Horas desde último refresh RAG",
      "gridPos": {"h": 4, "w": 4, "x": 20, "y": 0},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [
        {
          "expr": "(time() - max(rag_index_last_refresh_timestamp{store=~'$store'})) / 3600",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "h",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green"},
              {"color": "red", "value": 24}
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Throughput por status",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [
        {
          "expr": "sum(rate(api_requests_total{job=~'$job', entity=~'$entity', intent=~'$intent'}[5m])) by (status)",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {"showLegend": true},
        "tooltip": {"mode": "multi"}
      },
      "fieldConfig": {
        "defaults": {"unit": "req/s"},
        "overrides": []
      }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Latência p50/p95/p99",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(api_latency_ms_bucket{job=~'$job', entity=~'$entity', intent=~'$intent'}[5m])) by (le)) * 1000",
          "refId": "A",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(api_latency_ms_bucket{job=~'$job', entity=~'$entity', intent=~'$intent'}[5m])) by (le)) * 1000",
          "refId": "B",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(api_latency_ms_bucket{job=~'$job', entity=~'$entity', intent=~'$intent'}[5m])) by (le)) * 1000",
          "refId": "C",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green"},
              {"color": "red", "value": 1500}
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {"showLegend": true},
        "tooltip": {"mode": "multi"}
      }
    },
    {
      "id": 9,
      "type": "table",
      "title": "Top entidades por tráfego",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [
        {
          "expr": "topk(10, sum(rate(api_requests_total{job=~'$job', intent=~'$intent'}[15m])) by (entity))",
          "format": "table",
          "refId": "A"
        }
      ],
      "options": {
        "showHeader": true
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            }
          }
        }
      ]
    },
    {
      "id": 10,
      "type": "table",
      "title": "Top intents por erro",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
      "datasource": {"type": "prometheus", "uid": "-"},
      "targets": [
        {
          "expr": "topk(10, sum(rate(api_errors_total{job=~'$job', entity=~'$entity'}[15m])) by (intent))",
          "format": "table",
          "refId": "A"
        }
      ],
      "options": {
        "showHeader": true
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            }
          }
        }
      ]
    }
  ],
  "templating": {
    "list": [
      {
        "name": "job",
        "label": "Job",
        "type": "query",
        "datasource": {"type": "prometheus", "uid": "-"},
        "definition": "label_values(api_requests_total, job)",
        "query": "label_values(api_requests_total, job)",
        "refresh": 1,
        "includeAll": true,
        "multi": false,
        "current": {
          "selected": false,
          "text": "araquem-api",
          "value": "araquem-api"
        }
      },
      {
        "name": "entity",
        "label": "Entity",
        "type": "query",
        "datasource": {"type": "prometheus", "uid": "-"},
        "definition": "label_values(api_requests_total, entity)",
        "query": "label_values(api_requests_total, entity)",
        "refresh": 1,
        "includeAll": true,
        "multi": false,
        "current": {
          "selected": false,
          "text": "fiis_cadastro",
          "value": "fiis_cadastro"
        }
      },
      {
        "name": "intent",
        "label": "Intent",
        "type": "query",
        "datasource": {"type": "prometheus", "uid": "-"},
        "definition": "label_values(api_requests_total, intent)",
        "query": "label_values(api_requests_total, intent)",
        "refresh": 1,
        "includeAll": true,
        "multi": false,
        "current": {
          "selected": false,
          "text": "cadastro",
          "value": "cadastro"
        }
      },
      {
        "name": "store",
        "label": "Store",
        "type": "query",
        "datasource": {"type": "prometheus", "uid": "-"},
        "definition": "label_values(rag_index_docs_total, store)",
        "query": "label_values(rag_index_docs_total, store)",
        "refresh": 1,
        "includeAll": true,
        "multi": false,
        "current": {
          "selected": false,
          "text": "embeddings.jsonl",
          "value": "embeddings.jsonl"
        }
      },
      {
        "name": "interval",
        "type": "interval",
        "label": "Interval",
        "query": "5m,10m,15m,30m,1h"
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {"type": "-- Grafana --", "uid": "grafana"},
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  }
}
